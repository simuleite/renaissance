{
  "name": "outln",
  "version": "0.1.0",
  "type": "module",
  "description": "Extract code outline (symbols) from source files using tree-sitter",
  "bin": {
    "outln": "./dist/cli/main.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node build.js",
    "dev": "tsx src/cli/main.ts",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "tree-sitter": "^0.21.0",
    "tree-sitter-go": "^0.21.0",
