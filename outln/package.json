    "tree-sitter-python": "^0.21.0",
    "tree-sitter-java": "^0.21.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "esbuild": "^0.19.0",
